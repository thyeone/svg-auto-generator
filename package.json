{
  "name": "@thyeone/svg-sprite-generator",
  "version": "1.0.7",
  "description": "svg-sprite-generator",
  "author": "thyeone",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thyeone/svg-sprite-generator.git"
  },
  "bin": {
    "svg-init": "dist/init.js",
    "svg-generate": "dist/generate.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "mkdir -p dist && cp src/* dist/",
    "prepublishOnly": "npm run build",
    "release": "npm version patch && git push --tags"
  },
  "keywords": [
    "svg",
    "sprite",
    "react",
    "generator"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "svgstore": "^3.0.1",
    "svgstore-cli": "^2.0.1"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5"
  }
}
